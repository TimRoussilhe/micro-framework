{
  "name": "Boilerplate",
  "description": "Boilerplate framework",
  "version": "0.0.2",
  "private": true,
  "author": "Tim Roussilhe",
  "contributors": [
    "Tim Roussilhe"
  ],
  "app": {},
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "scripts": {
    "dist:sass": "node-sass --include-path node_modules ./src/css/app.scss ./build/assets/css/app.min.css",
    "dist:autoprefixer": "postcss -c postcss.config.js ./build/assets/css/app.min.css --replace --no-map",
    "dist:js": "webpack --config ./config/webpack.prod.config.js ",
    "lint:css": "stylelint ./build/assets/css/*.css",
    "lint:js": "eslint ./src/scripts/project",
    "watch:js": "webpack --config ./config/webpack.dev.config.js --progress --colors --watch --cache",
    "svgs-to-jsons": "gulp svgs-to-jsons",
    "server": "php -S localhost:8000 -t public/",
    "browsersync": "gulp browserSync",
    "build": "gulp build",
    "dev": "concurrently -p \"[{name}]\" -n \"WEBPACK\" -c \"bgBlue.bold,bgRed.bold\" \"npm run watch:js\"",
    "start": "npm run server & npm run browsersync & npm run watch:js",
    "deploy": "npm run dist:sass && npm run dist:autoprefixer && npm run dist:js && npm run lint:js && npm run build "
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "concurrently": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gsap": "^1.20.6",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-connect-php": "^1.0.3",
    "gulp-files-to-json": "^0.2.1",
    "gulp-imagemin": "^3.4.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svgmin": "^1.2.4",
    "gulp-watch": "^4.3.11",
    "imports-loader": "^0.7.1",
    "is_js": "^0.9.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.10",
    "node-notifier": "^5.2.1",
    "normalize.css": "^7.0.0",
    "page": "^1.8.6",
    "redux": "^3.7.2",
    "redux-responsive": "^4.3.7",
    "redux-watch": "^1.1.1",
    "run-sequence": "^2.2.1",
    "twig-loader": "^0.3.1",
    "webpack": "^3.12.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-manifest-plugin": "^1.3.2",
    "zepto": "^1.2.0"
  },
  "dependencies": {
    "redux-logger": "^3.0.6"
  }
}
